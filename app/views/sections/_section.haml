= marker_html(section, @marker_options)
- if section.title
  = @title = section.title
  %h2
    %a{ :name => section.title_for_linking, :href => '#' + section.title_for_linking }
      = section.title_cleaned_up
- if section.is_a? OralQuestionsSection and section.introduction
  %p.question_introduction= section.introduction.text
= render :partial => 'contribution', :collection => section.contributions
= render :partial => 'section', :collection => section.sections
