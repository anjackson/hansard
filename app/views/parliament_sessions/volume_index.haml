- @title = "#{@series_number.titleize} Series, Volume #{@volume_number}"

- if @commons_session
  %h2 House of Commons
  - @commons_session.sittings.each do |sitting|
    .sitting_columns
      %p= sitting.date_text
      %p= sitting_column_links(sitting)
- if @lords_session
  %h2 House of Lords
  - @lords_session.sittings.each do |sitting|
    .sitting_columns
      %p= sitting.date_text
      %p= sitting_column_links(sitting)