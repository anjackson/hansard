- warning = warning_reloaded
%td= warning.directory.split('/').last+'.xml'
%td= warning.type_of_data
%td= warning.date_text
%td= 'reloaded: ' + h(warning.log)
%td= warning.saved ? 'Y' : 'N'
- if warning.reload_possible?
  - form_remote_tag :url => { :controller => 'data_files', :action => 'reload_commmons_for_date', :date => warning.date.to_s } do
    %td= submit_tag "reload"